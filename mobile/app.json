{
  "expo": {
    "name": "Digital Athenaeum",
    "slug": "athenaeum",
    "scheme": "athenaeum",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "experiments": {
      "turboModules": true
    },
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.anonymous.athenaeum"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-secure-store",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow Digital Athenaeum to access your camera to scan book pages for quotes."
        }
      ],
      [
        "rn-mlkit-ocr",
        {
          "ocrModels": ["latin"],
          "ocrUseBundled": true
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "newArchEnabled": true,
            "packagingOptions": {
              "pickFirst": ["**/libc++_shared.so"]
            }
          },
          "ios": {
            "newArchEnabled": true
          }
        }
      ]
    ]
  }
}
